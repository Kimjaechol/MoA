# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Admin
ADMIN_SECRET=your-admin-secret

# Encryption key for API keys (AES-256-GCM) and relay tokens
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
MOA_ENCRYPTION_KEY=your-64-char-hex-key

# MoA public-facing URL (shown to users in install commands)
NEXT_PUBLIC_API_URL=https://mymoa.app

# Railway backend URL (Vercel rewrites proxy /install*, /api/* here)
# Set this in Vercel env vars to your Railway deployment domain
RAILWAY_BACKEND_URL=https://your-moa-project.up.railway.app
